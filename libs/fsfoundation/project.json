{
  "root": "libs/fsfoundation",
  "sourceRoot": "libs/fsfoundation/src",
  "projectType": "library",
  "tags": ["open-source"],
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/fsfoundation",
        "tsConfig": "libs/fsfoundation/tsconfig.lib.json",
        "packageJson": "libs/fsfoundation/package.json",
        "main": "libs/fsfoundation/src/index.ts",
        "assets": ["libs/fsfoundation/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/fsfoundation/**/*.{ts,tsx,js,jsx}"],
        "hasTypeAwareRules": true
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/fsfoundation"],
      "options": {
        "jestConfig": "libs/fsfoundation/jest.config.js",
        "passWithNoTests": true
      }
    },
    "publish": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  }
}
