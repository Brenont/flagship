{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "cli": "nx",
  "properties": {
    "repo": {
      "type": "string",
      "description": "The repo where to pull the filtered tree from"
    },
    "pullRequestNumber": {
      "type": "string",
      "description": "The pull request to pull"
    },
    "dryRun": {
      "type": "boolean",
      "description": "Wether a pull request should be created when complete"
    }
  },
  "required": ["repo", "pullRequestNumber"]
}
